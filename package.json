{
  "name": "pkg-name",
  "version": "0.0.0",
  "packageManager": "pnpm@7.0.1",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/hyhan/pkg-name#readme",
  "bugs": {
    "url": "https://github.com/hyhan/pkg-name/issues"
  },
  "license": "MIT",
  "author": "Han Hui <hanhui858@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyhan/pkg-name.git"
  },
  "funding": "https://github.com/sponsors/hyhan",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.14.0",
    "@babel/types": "^7.18.4",
    "@hyhan/eslint-config": "^1.0.0",
    "@types/node": "^17.0.38",
    "bumpp": "^7.1.1",
    "eslint": "^8.16.0",
    "esno": "^0.14.1",
    "pnpm": "^6.32.19",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2",
    "unbuild": "^0.7.4",
    "vite": "^2.9.9",
    "vitest": "^0.9.4"
  }
}
